package sqldb

import (
	"fmt"

	_ "github.com/go‚Äêsql-driver/mysql"
)

type DB struct {
	db sql.Db
}

type DbConfig struct {
	UName    string
	Password string
	Host     string
	Port     string
	Db       string
}

var conFmt string = "%s:%s@tcp(%s:%s)/%s"

func NewDB(con DbConfig) (*DB, error) {
	conString := fmt.Sprintf(conFmt, con.UName, con.Password, con.Host, con.Port, con.Db)
	db, err := sql.Open("mysql", conString)
	if err != nil {
		return nil, nil
	}
	return
}
